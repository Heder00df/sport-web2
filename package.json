{
  "name": "react-redux-starter-kit",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "lodash": "4.17.11",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.5",
    "react-select": "2.0.0",
    "redux": "3.7.2",
    "redux-form": "7.4.2",
    "redux-form-material-ui": "5.0.0-beta.3",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "standard --fix | snazzy",
    "build": "react-scripts build",
    "test": "npm run lint && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sonar": "npm run coverage && ./sonarqube/bin/sonar-scanner",
    "coverage": "CI=true react-scripts test --env=jsdom --collectCoverage true --coverageDirectory ./.coverage"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.15.0",
    "@babel/code-frame": "7.0.0",
    "@babel/traverse": "7.0.0",
    "@babel/types": "7.0.0",
    "babel-eslint": "8.2.6",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "react-test-renderer": "16.5.2",
    "redux-mock-store": "1.5.3",
    "snazzy": "7.1.1",
    "standard": "11.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "jest": true
    },
    "ignore": [
      "/src/registerServiceWorker.js"
    ],
    "globals": [
      "localStorage",
      "mockReduxStore"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
